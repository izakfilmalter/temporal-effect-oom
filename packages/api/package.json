{
  "name": "@if/api",
  "version": "1.0.0",
  "description": "A TypeScript-based Express starter template with auto file routing and ESLint integration. Build scalable, modular apps with ease.",
  "main": "index.js",
  "type": "module",
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "module": "ESNext"
    }
  },
  "scripts": {
    "lint": "eslint",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "ship": "pnpm build && vercel --prod",
    "v:login": "vercel login",
    "setup:eslint": "npx eslint --init"
  },
  "keywords": [],
  "dependencies": {
    "@if/tsconfig": "*",
    "effect": "3.11.8",
    "express": "^4.21.0",
    "express-file-routing": "^3.0.3",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@unocss/eslint-config": "^0.52.2",
    "eslint": "9.4.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-import": "^2.27.5",
    "globals": "^15.9.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "4.7.0",
    "typescript": "^5.6.0",
    "vercel": "^37.5.4",
    "vitest": "^2.1.1",
    "yml": "^1.0.0"
  }
}
