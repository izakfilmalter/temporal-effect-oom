{
  "name": "@if/workers",
  "version": "0.1.0",
  "private": true,
  "main": "lib/index.js",
  "typings": "index.ts",
  "scripts": {
    "lint": "eslint",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit",
    "build": "rm -rf ./lib && tsx scripts/build.ts && tsx scripts/build-workflow-bundle.ts",
    "start": "node  --inspect ./lib/workers/worker.js",
    "dev:workers": "tsx watch --inspect --heapsnapshot-near-heap-limit=3 workers/worker.ts"
  },
  "sideEffects": false,
  "dependencies": {
    "@clerk/backend": "1.18.0",
    "@effect/platform": "0.71.2",
    "@effect/platform-node": "0.42.9",
    "@if/api": "*",
    "@if/tsconfig": "*",
    "@sentry/node": "7.93.0",
    "@sentry/tracing": "7.93.0",
    "@steepleinc/effect-http": "0.83.1",
    "@temporalio/activity": "1.11.5",
    "@temporalio/worker": "1.11.5",
    "@temporalio/workflow": "1.11.5",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.36.3",
    "effect": "3.11.8",
    "iovalkey": "0.2.1",
    "mime": "3.0.0",
    "module-alias": "2.2.2",
    "nanoid": "5.0.9",
    "qs": "6.11.2",
    "stream-mime-type": "2.0.0",
    "triple-beam": "1.4.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/mime": "3.0.4",
    "@types/module-alias": "2.0.4",
    "@types/qs": "6.9.8",
    "@types/triple-beam": "1.3.5",
    "esbuild": "0.20.0",
    "eslint": "9.4.0",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.8",
    "tsx": "4.7.0",
    "typescript": "5.7.2"
  }
}
